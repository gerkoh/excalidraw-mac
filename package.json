{
  "name": "excalidraw-mac",
  "version": "1.0.0",
  "description": "Wrapper around excalidraw with local auto-save functionality",
  "license": "MIT",
  "author": "Gerald Koh",
  "type": "commonjs",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "vite dev",
    "build": "vite build",
    "dmg": "electron-builder",
    "compile": "npm run build && npm run dmg"
  },
  "dependencies": {
    "@excalidraw/excalidraw": "^0.18.0",
    "@vitejs/plugin-react": "^5.1.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "electron": "^40.1.0",
    "electron-builder": "^26.7.0",
    "vite": "^7.3.1"
  },
  "build": {
    "appId": "com.geraldkoh.excalidrawmac",
    "mac": {
      "target": "dmg"
    }
  }
}
